<?xml version="1.0" encoding="utf-8"?>
<launch>
  <arg name="launch_rqtools" default="true"/>

  <group if="$(arg launch_rqtools)">
    <!--<node pkg="rqt_console" type="rqt_console" name="rqt_console"/> -->
    <node pkg="rviz" type="rviz" name="rviz" 
          args="--fixed-frame topocentric -d $(find starleth_tr)/conf/icp_nav.rviz"/>
    <!--node pkg="dynamic_reconfigure" type="reconfigure_gui" name="reconfigure_gui" / -->
  </group>

  <node name="velodyne_post_op" pkg="velodyne_post" type="velodyne_post_node" output="screen" respawn="true">
    <rosparam command="load" file="$(find velodyne_post)/etc/velodyne32_post.yaml"/>
    <param name="sensor/calibration_file" value="$(find velodyne_post)/etc/calib-HDL-32E.dat"/>
    <param name="ros/use_binary_snappy" value="true"/>
  </node>
</launch>
